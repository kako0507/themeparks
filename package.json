    "author": {
        "name": "Jamie Holding",
        "email": "me@jamie.holdings"
    },
    "dependencies": {
        "babel-eslint": "^8.2.3",
        "bluebird": "^3.5.1",
        "cache-manager": "^2.6.0",
        "cache-manager-memory-store": "^1.1.0",
        "cheerio": "^1.0.0-rc.2",
        "cookie": "^0.3.1",
        "https-proxy-agent": "^2.2.0",
        "lodash": "^4.17.10",
        "moment": "^2.20.1",
        "moment-timezone": "^0.5.14",
        "needle": "2.1.2",
        "random-useragent": "^0.3.1",
        "relaxed-json": "^1.0.1",
        "socks-proxy-agent": "^3.0.1",
